{
  "name": "tabEQ",
  "description": "Audio controls for your tabs!",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": ["storage"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_start"
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["page.js"],
      "matches": ["<all_urls>"]
    }
  ],
  "action": {
    "default_popup": "popup/index.html",
    "default_icon": {
      "16": "images/tabEQ_16.png",
      "32": "images/tabEQ_32.png",
      "48": "images/tabEQ_48.png",
      "128": "images/tabEQ_128.png"
    }
  },
  "icons": {
    "16": "images/tabEQ_16.png",
    "32": "images/tabEQ_32.png",
    "48": "images/tabEQ_48.png",
    "128": "images/tabEQ_128.png"
  }
}
